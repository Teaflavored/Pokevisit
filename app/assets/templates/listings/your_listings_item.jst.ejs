<div class="flipbook">
  <div class="front">
    <% if (image) { %>
      <img class="flipimages" src='<%= image.escape("url") %>'>
    <% } else { %>
      <img class="flipimages" alt="IMG">
    <% } %>
  </div>
  <div class="back">
    <div class="your-listing-location">
      <div class="listing-description"><strong>Address:asdfasdfasdfasdfasdfasdfasdfasdfasdfasdfasdfadfasdfasfdsafdasfs</strong> <%= listing.escape("address")%> </div>
    </div>

    <div class="your-listing-avail">
      <% var dateAvail = new Date(listing.get("date_avail")) %>
      <% dateAvail = (dateAvail.getMonth() + 1 + '/' + dateAvail.getDate() + '/' +  dateAvail.getFullYear()) %>

      <div class="listing-description"><strong>Date Available:</strong> <%= dateAvail %></div>
    </div>

    <div class="your-listing-end">
      <% var dateEnd = new Date(listing.get("date_end")) %>
      <% dateEnd = (dateEnd.getMonth() + 1 + '/' + dateEnd.getDate() + '/' +  dateEnd.getFullYear()) %>

      <div class="listing-description"><strong>Date End:</strong> <%= dateEnd %> </div>
    </div>

    <button class="btn btn-special edit-listing">Edit Listing</button>

    <button class="btn btn-special show-reservations">Show Reservations</button>

  </div>
</div>
